class Main

{

 static int i, n , count; 

  

public static void main(String args[])

{

 for(n=1; n<= 100;n++)

  {

 count = 0;

 for(i=2;i <= n/2;i++)

{

if(n%i == 0)

{

count++;

break;

  }

  }	 

if(count == 0 && n != 1 )

System.out.print(n +" ");

 }

}

} 
